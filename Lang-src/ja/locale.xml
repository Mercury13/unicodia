	<!-- You can make any wiki templates (w/o params) you want. Maybe you’d like to enclose them in CDATA. -->
	<wiki-templates>
		<!-- first{{-}}second  →  typographic dash -->
		<tmpl name="-"><![CDATA[<span style='font-size:4pt'>&#xA0;</span>—<span style='font-size:4pt'> </span>]]></tmpl>
		<!-- first{{,-}}second  →  typographic comma-dash -->
		<tmpl name=",-"><![CDATA[,—<span style='font-size:4pt'> </span>]]></tmpl>
	</wiki-templates>
	<icons sort-az="sort_kana" />
	<num-format dec-point="." thou-point="," thou-min-length="4" thou-min-length-dense="4">
		<imprecise>
			<!-- Bigger units specially for Japanese:
				 instead of “2,400k” you write “2M400k”.
				 Program’s limitation: bigger units are never written in ranges -->
			<fmt shift="0" text="{1}人" frac="never" />
			<fmt shift="3" text="{1}千人" frac="avoid" />
			<fmt shift="4" text="{1}万人" frac="never" bigger-unit="億" bigger-subshift="4" />
			<fmt shift="7" text="{1}千万人" frac="except1" bigger-unit="億" bigger-subshift="1" />
			<fmt shift="8" text="{1}億人" frac="never" />
		</imprecise>
		<moreless less="{1}以下" more="{1}以上" phase="unit" />
	</num-format>
	<!-- Japanese has no plural rules -->
	<cardinal-rules default-outcome="other" />
	<!-- Misc. language peculiarities
			still-uses-burmese: uses pair Myanmar/Burmese
				(writes location of Myanmar for Burmese script) -->
	<peculiarities still-uses-burmese="0" />
	<punctuation key-value-colon="： " uniform-comma="、" semicolon="；"
			left-paren="（" right-paren="）" indent-ellip="…"
			range="〜" year-range="〜" />
